@using Dtos;

<table class="table">
    <thead>
        <tr>
            <th>FirstName</th>
            <th>LastName</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Students)
        {
            <tr>
                <td><a>@student.FirstName</a></td>
                <td><a>@student.LastName</a></td>
                <td><a>@student.Email</a></td>
            </tr>
        }
    </tbody>
</table>

@code {
    [Parameter] public IEnumerable<ReadStudentDto> Students{ get; set; }
}
